  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "npm run build && npx source-map-explorer 'build/**/*.js'",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
